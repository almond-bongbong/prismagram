{
  "name": "prismagram",
  "version": "1.0.0",
  "description": "Instagram clone with Express + Prisma + React Native",
  "main": "index.js",
  "repository": "https://github.com/almond-bongbong/prismagram.git",
  "author": "cmlee <bal.dongdongdong@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node src/server.js",
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "yarn deploy && yarn generate",
    "prebuild": "yarn run generate",
    "build": "babel src -d build",
    "postbuild": "./node_modules/.bin/copy src/api/**/*.graphql ./build/api/",
    "start": "node build/server.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "aws-sdk": "^2.814.0",
    "dotenv": "^8.2.0",
    "expo-facebook": "^8.1.0",
    "graphql-tools": "^5.0.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "merge-graphql-schemas": "^1.7.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.34.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.9.5",
    "copy": "^0.3.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4"
  }
}
